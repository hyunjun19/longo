    "express-ipfilter": "^0.1.1",
    "lodash": "^4.16.2",
    "monk": "^3.1.2",
    "morgan": "~1.7.0",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "~2.3.0"
  }
}
